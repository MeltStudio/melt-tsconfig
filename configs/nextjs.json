{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "incremental": true,
    "jsx": "preserve",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "ESNext",
    "moduleResolution": "node",
    "noEmit": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "target": "ES5"
  },
  "include": ["next-env.d.ts", "src", ".next/types/**/*.ts"]
}
